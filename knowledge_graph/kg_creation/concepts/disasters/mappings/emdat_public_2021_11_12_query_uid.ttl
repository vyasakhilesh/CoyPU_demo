        rr:tableName "emdat_public_2021_11_12_query_uid"
    ];
    rr:subjectMap [
        rr:template "https://data.coypu.org/disasters/{Dis No}";
        rr:class ips:Event;
    ];

    rr:predicateObjectMap [
        rr:predicate ips:year;
        rr:objectMap [ rml:reference "Year"; rr:datatype xsd:positiveInteger];
    ];

    rr:predicateObjectMap [
        rr:predicate ips:year;
        rr:objectMap [ rml:reference "Start Year"; rr:datatype xsd:positiveInteger];
    ];

    rr:predicateObjectMap [
        rr:predicate ips:year;
        rr:objectMap [ rml:reference "End Year"; rr:datatype xsd:positiveInteger];
    ];

    rr:predicateObjectMap [
        rr:predicate ex:hasEventID;
