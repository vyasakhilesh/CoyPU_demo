    ];

    rr:predicateObjectMap [
        rr:predicate ips:locatedAt;
        rr:objectMap [ rr:template "https://data.coypu.org/countries/{ISO}"];
    ];
    
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [ rml:reference "Event Name"; rr:language "en"];
    ].

