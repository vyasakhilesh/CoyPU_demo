<file:///knowledge_graph/kg_creation/countries/mappings/CountriesResilienceSample2021_2>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/knowledge_graph/kg_creation/countries/mappings/translated/CountriesResilienceSample2021_csv_func_PROJECT1.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://example.com/ns#CountryStatistics/{iso}";
    ];
    rr:predicateObjectMap [
        rr:predicate ips:exposure;
        rr:objectMap [
        rr:parentTriplesMap <chomp_output1>;
        rr:joinCondition [
            rr:child "Exposure";
            rr:parent "Exposure";
            ];
        ];
    ];
    rr:predicateObjectMap [
        rr:predicate ips:year;
        rr:objectMap [
        rr:constant "2021"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate ips:riskLevel;
        rr:objectMap [
        rml:reference "Rang"
        ]
    ].

<CountriesResilienceSample2021_1>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/knowledge_graph/kg_creation/countries/mappings/translated/CountriesResilienceSample2021_csv_func_PROJECT2.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "https://data.coypu.org/countries/{iso}";
        rr:class ips:Country
    ];
    rr:predicateObjectMap [
        rr:predicate rdfs:alternateName;
        rr:objectMap [
        rml:reference "country_name_de"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [
        rml:reference "country_name_en"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate ex:hasCountryStatistics;
        rr:objectMap [
        rr:template  "http://example.com/ns#CountryStatistics/{iso}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate ips:isoCode;
        rr:objectMap [
        rml:reference "iso"
        ]
    ].

<chomp_output1>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/knowledge_graph/kg_creation/countries/mappings/translated/CountriesResilienceSample2021_csv_func_OUTPUT1.csv";
                rml:referenceFormulation ql:CSV
            ];
    rr:subjectMap [
        rml:reference "chomp_output1";
        rr:termType rr:IRI
    ].

