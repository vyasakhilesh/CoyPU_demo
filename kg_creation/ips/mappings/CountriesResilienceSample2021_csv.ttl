@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix ips: <https://schema.implisense.com/#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <https://schema.org/> .
@prefix ex: <http://example.com/ns/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .

<CountriesResilienceSample2021_2> a rr:TriplesMap;
    rml:logicalSource [ rml:source "/home/tibvyasa/projects/proj_off/data_off/coypu/ips/data/CountriesResilienceSample2021.csv";
						rml:referenceFormulation ql:CSV 
						];
    
    rr:subjectMap [
        rr:template "http://example.com/ns#CountryStatistics/{iso}";
    ];

    rr:predicateObjectMap [
        rr:predicate ips:riskLevel;
        rr:objectMap [ rml:reference "Rang"; rr:datatype xsd:positiveInteger];
    ];
    
     rr:predicateObjectMap [
        rr:predicate ips:year;
        rr:objectMap [ rr:constant "2021"; rr:datatype xsd:positiveInteger];
    ];

    rr:predicateObjectMap [
        rr:predicate ips:exposure;
        rr:objectMap <#chomp_function>
    #    rml:reference "Exposure"
    ].

<#CountriesResilienceSample2021_1> a rr:TriplesMap;
    rml:logicalSource [ rml:source "/home/tibvyasa/projects/proj_off/data_off/coypu/ips/data/CountriesResilienceSample2021.csv";
						rml:referenceFormulation ql:CSV 
						];
    
    rr:subjectMap [
        rr:template "https://data.coypu.org/countries/{iso}";
        rr:class ips:Country;
    ];

    rr:predicateObjectMap [
        rr:predicate ips:isoCode;
        rr:objectMap [ rml:reference "iso"; rr:datatype xsd:string];
    ];

    rr:predicateObjectMap [
        rr:predicate rdfs:label;
        rr:objectMap [ rml:reference "country_name_en"; rr:language "en"];
    ];

    rr:predicateObjectMap [
        rr:predicate rdfs:alternateName;
        rr:objectMap [ rml:reference "country_name_de"; rr:language "de"];
    ];
    
    rr:predicateObjectMap [
        rr:predicate ex:hasCountryStatistics;
        rr:objectMap [rr:template "http://example.com/ns#CountryStatistics/{iso}"];
       # rr:objectMap  <RefObjectMap1>
    ].

